rops.createRoom(room)\n\n   }\n\n   handleChange(e){\n      this.setState({\n         newTitle: e.currentTarget.value,\n      })\n   }\n\n\n   render(){\n\n      let rooms = this.props.rooms || {};\n      let roomIds = [];\n      debugger;\n\n\n\n      Object.keys(rooms).forEach(key => {\n         roomIds.push(rooms[key]._id);  \n      })\n\n      return(\n         <div>\n            <form onSubmit={this.createNewRoom}>\n               <input type=\"text\" value={this.state.newTitle} \n                        onChange={this.handleChange}\n                        placeholder=\"Enter new room title\"/>\n\n            </form>\n            <button onClick={this.createNewRoom}>Create a New Chat Room</button>\n            <div>\n               {\n                  roomIds.map(id=>(\n                     <ChatBox roomId={id} key={id} />\n                  ))\n               }  \n            </div>\n\n         </div>\n      )\n   }\n}\n\n\nexport default DashBoard;",{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":2,"column":21,"nodeType":"137","messageId":"138","endLine":2,"endColumn":35},{"ruleId":"139","severity":1,"message":"140","line":10,"column":1,"nodeType":"141","endLine":32,"endColumn":2},{"ruleId":"135","severity":1,"message":"142","line":1,"column":28,"nodeType":"137","messageId":"138","endLine":1,"endColumn":47},{"ruleId":"135","severity":1,"message":"143","line":3,"column":9,"nodeType":"137","messageId":"138","endLine":3,"endColumn":17},{"ruleId":"135","severity":1,"message":"144","line":1,"column":10,"nodeType":"137","messageId":"138","endLine":1,"endColumn":22},{"ruleId":"135","severity":1,"message":"143","line":4,"column":9,"nodeType":"137","messageId":"138","endLine":4,"endColumn":17},{"ruleId":"135","severity":1,"message":"145","line":5,"column":10,"nodeType":"137","messageId":"138","endLine":5,"endColumn":15},{"ruleId":"146","severity":1,"message":"147","line":28,"column":37,"node